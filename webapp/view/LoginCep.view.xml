
<mvc:View xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m" controllerName="logincep.controller.LoginCep" 
    displayBlock="false" >
    <Page id="page" >
        <content >
            <FlexBox class= "flexBoxTLG" id="_IDGenFlexBox1"  >
                <VBox class="myVBox1" id="_IDGenVbox1" >


                    
                    <VBox id="body" class= "myVboxBody">
                    

                    <FlexBox id="1" class= "FlextextLogin" >   
                        <Button id="_IDGenButton3" icon="sap-icon://customer" class= "icon"  />
                    </FlexBox>

                    <FlexBox id="buttonsID" class= "buttonsForm">


                            <Input id="_IDGenInput1" showClearIcon="true" placeholder="Digite deu email" width="18rem" class="myInput"/>
                            <Input  id="_IDGenInput2"  type="Password" showValueHelp="true" showClearIcon="false" placeholder="Digite sua senha" width="18rem"/>

                    </FlexBox>

                    <FlexBox id="IDFlexBox2" class= "buttonsLogin">

                        <Button id="_IDGenButton1" text="Login" type="Accept" press="OnLogin" class= "button" />
                        <Button id="_IDGenButton2" text="Registre" type="Reject" press="onRegister" class= "button"/>

                    </FlexBox>

                    </VBox>
                </VBox>
            </FlexBox>
        </content>
    </Page>
</mvc:View>
