<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout" controllerName="logincep.controller.LoginCep" displayBlock="true">
    <FlexBox
        width="100%"
        height="100%"
        alignItems="Center"
        justifyContent="Center"
        >
        
        <l:VerticalLayout
            id="_IDGenVerticalLayout1"
            class="centerDiv sapUiContentPadding"
            width="100%"
        >
            <l:content>
                <Label text="UserName" labelFor="InputDisabled"/>
                <Input
                    id="_IDGenInput1"
                    class="sapUiSmallMarginBottom"
                    type="Text"
                    placeholder="UserName"
                    enabled="true"
                    editable="true"
                />
                <Label text="Password" labelFor="InputEdit"/>
                <Input
                    id="_IDGenInput2"
                    class="sapUiSmallMarginBottom"
                    type="Password"
                    
                    placeholder="Enter password"
                    enabled="true"
                    editable="true"
                />

                <FlexBox id="IDFlexBox2" justifyContent="Center">
                    <Button id="_IDGenButton1" text="Login" type="Accept" press="OnLogin" class="buttonCss sapUiSmallMargin"/>
                    <Button id="_IDGenButton2" text="Registre" type="Reject" press="onRegister" class="buttonCss sapUiSmallMargin"/>
                </FlexBox>
            </l:content>
        </l:VerticalLayout>
    </FlexBox>

    
